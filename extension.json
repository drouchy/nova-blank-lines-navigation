{
    "identifier": "dev.rouchy.NavigateBetweemBlankLines",
    "name": "Navigate Between Blank Lines",
    "organization": "Rouchy",
    "description": "Move the editor cursor to the next / previous blank line",
    "version": "1.1",
    "categories": ["commands"],
    
    "repository": "https://github.com/drouchy/nova-blank-lines-navigation",
    "bugs": "https://github.com/drouchy/nova-blank-lines-navigation/issues",
      
    "main": "main.js",
    
    "activationEvents": [
        "onLanguage:*"
    ],
    
    "entitlements": {
        
    },
    
    "commands": {
        "editor": [
            {
                "title": "Move to next blank line",
                "command": "navigate-betweem-blank-lines.next",
                "shortcut": "ctrl-command-down",
                "when": "editorHasFocus"
            },
            {
                "title": "Move to previous blank line",
                "command": "navigate-betweem-blank-lines.previous",
                "shortcut": "ctrl-command-up",
                "when": "editorHasFocus"
            }
        ]
    },
    
    "config": [
    ]
}
